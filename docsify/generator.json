{
	"version": "0.1.0",
	"generator": 
	{
		"name": "Docsify",
		"version": "0.2.1",
		"maintainer": "Mark Dumay <mark@markdumay.com>",
		"origin": "https://github.com/markdumay/shelldoc-generators/docsify",
		"description": "Docsify is a documentation site generator that generates content on the fly. It smartly loads and parses Markdown files and displays them as a website. More information is available on https://docsify.js.org.",
		"path": "$workspaceRoot/generators/docsify",
		"outDir": "docs/",
		"themeColor": "",
		"logo": "",
		"templates": [
			{
				"source": "coverpage.tmpl",
				"dest": "_coverpage.md",
				"format": "project"
			},
			{
				"source": "sidebar.tmpl",
				"dest": "_sidebar.md",
				"format": "project"
			},
			{
				"source": "index.tmpl",
				"dest": "index.html",
				"format": "project"
			},
			{
				"source": "page.tmpl",
				"dest": "${name}.md",
				"format": "file"
			}
		],
		"artifacts": [
			{
				"source": ".nojekyll",
				"dest": ".nojekyll"
			}
		]
	}
}